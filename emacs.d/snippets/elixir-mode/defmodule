# -*- mode: snippet -*-
# name: defmodule for struct
# key: defmodule
# --
defmodule ${1:name} do
  @moduledoc """
  """

  @type t :: %__MODULE__{
  }

  defstruct [
  ]


  #####################################################
  # Public API
  #####################################################

  @doc """
  Creates a new struct using the passed keyword or map.

  """
  @spec new(keyword | map) :: t
  def new(fields), do: struct(__MODULE__, fields)

end